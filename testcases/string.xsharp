class String{
    char[] data;
    i64 size;
}

String newStr(i64 size){
    String str = new String;
    str.data = new char[](size);
    str.size = size;
    return str;
}

i32 main(){
    String str = newStr(3);
    i32 i =0;
    str.data = "123";
    print(str.data);
    while (i < str.size)
    {
        print(str.data[i]);
        i=i+1;
    }
    return 0;
}
