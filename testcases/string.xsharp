class String{
    char[] data;
    i64 size;

    void init(){
        self.data = new char[](1024);
        self.size = 1024;
        return;
    }
}

i32 main(){
    String str = newStr(3);
    str.init();
    i32 i =0;
    str.data = "123";
    print(str.data);
    while (i < str.size)
    {
        print(str.data[i]);
        i=i+1;
    }
    return 0;
}
